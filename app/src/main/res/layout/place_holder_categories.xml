<?xml version="1.0" encoding="utf-8"?>
<aglibs.loading.skeleton.layout.SkeletonConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="@dimen/_130sdp"
    android:layout_height="@dimen/_140sdp"
    app:cardCornerRadius="@dimen/_10sdp"
    android:layout_marginEnd="@dimen/_6sdp"
    app:cardUseCompatPadding="true">

    <aglibs.loading.skeleton.layout.SkeletonConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="@dimen/_8sdp">



        <aglibs.loading.skeleton.view.SkeletonTextView
            android:id="@+id/sName"
            style="@style/mediumText1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Medical Ser"
            android:textColor="@color/white"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <aglibs.loading.skeleton.view.SkeletonImageView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:scaleType="fitCenter"
            android:padding="@dimen/_10sdp"
            android:layout_marginTop="@dimen/_10sdp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/sName"
            android:id="@+id/sImage"/>


    </aglibs.loading.skeleton.layout.SkeletonConstraintLayout>

</aglibs.loading.skeleton.layout.SkeletonConstraintLayout>